.mui-checkbox-wrapper {
  position: relative;
  display: inline-flex;
  align-items: center;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}
.mui-checkbox-wrapper.mui-checkbox-wrapper-left {
  margin-left: 16px;
}
.mui-checkbox-wrapper.mui-checkbox-wrapper-right {
  margin-right: 16px;
}
.mui-checkbox-wrapper .mui-checkbox {
  padding: 7px;
  border-radius: 50%;
  box-sizing: border-box;
}
