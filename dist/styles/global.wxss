.mui {
  font-family: "Roboto", "Helvetica", "Arial", sans-serif;
  box-sizing: border-box;
  -webkit-font-smoothing: antialiased;
}
.mui-ripple-view {
  overflow: hidden;
  position: relative;
}
.mui-ripple-view .mui-ripple {
  position: absolute;
  border-radius: 100%;
  background-color: #000000;
  opacity: 0.3;
}
.mui-ripple-view .mui-ripple-animation {
  animation: ripple 0.6s ease-out;
  animation-fill-mode: forwards;
}
.mui-ripple-view .mui-ripple-animation-hold {
  animation: ripple-hold 0.6s ease-out;
  animation-fill-mode: forwards;
}
.mui-ripple-view .mui-ripple-animation-center {
  animation: ripple-center 0.6s ease-out;
  animation-fill-mode: forwards;
}
.mui-ripple-view .mui-ripple-animation-hold-center {
  animation: ripple-hold-center 0.6s ease-out;
  animation-fill-mode: forwards;
}
.mui-inline-flex {
  display: inline-flex;
}
.mui-block {
  display: block;
}
@keyframes ripple {
  from {
    transform: scale(0);
    opacity: 0.3;
  }
  to {
    transform: scale(2);
    opacity: 0;
  }
}
@keyframes ripple-hold {
  from {
    transform: scale(0);
    opacity: 0.3;
  }
  to {
    transform: scale(2);
    opacity: 0.1;
  }
}
@keyframes ripple-center {
  0% {
    transform: scale(0);
    opacity: 0.3;
  }
  40% {
    transform: scale(1);
    opacity: 0.12;
  }
  100% {
    transform: scale(1);
    opacity: 0;
  }
}
@keyframes ripple-hold-center {
  0% {
    transform: scale(0);
    opacity: 0.3;
  }
  40% {
    transform: scale(1);
    opacity: 0.12;
  }
  100% {
    transform: scale(1);
    opacity: 0.1;
  }
}
