.mui-button {
  position: relative;
  overflow: hidden;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  margin: 0;
  min-width: 64px;
  box-sizing: border-box;
  transition: background-color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, box-shadow 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms, border 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
  font-weight: 500;
  line-height: 1.75;
  letter-spacing: 0.4px;
  border: 0;
  border-radius: 4px;
  outline: 0;
  user-select: none;
  vertical-align: middle;
  text-decoration: none;
  appearance: none;
  -webkit-tap-highlight-color: transparent;
}
.mui-button::after {
  border: 0;
  border-radius: 0;
}
.mui-button.mui-button-hover {
  box-shadow: 0px 2px 4px -1px rgba(0, 0, 0, 0.2), 0px 4px 5px 0px rgba(0, 0, 0, 0.14), 0px 1px 10px 0px rgba(0, 0, 0, 0.12);
}
.mui-button.mui-button-contained {
  color: rgba(0, 0, 0, 0.87);
  background-color: #e0e0e0;
  box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2), 0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);
}
.mui-button.mui-button-contained.mui-button-hover {
  background-color: #d5d5d5;
}
.mui-button.mui-button-default.mui-button-outlined {
  border-color: rgba(0, 0, 0, 0.23);
}
.mui-button.mui-button-default.mui-button-outlined.mui-button-hover {
  background-color: rgba(0, 0, 0, 0.08);
}
.mui-button.mui-button-default.mui-button-text.mui-button-hover {
  background-color: rgba(0, 0, 0, 0.08);
}
.mui-button.mui-button-primary {
  color: #fff;
  background-color: #1976d2;
}
.mui-button.mui-button-primary.mui-button-hover {
  background-color: #115293;
}
.mui-button.mui-button-primary.mui-button-outlined {
  color: #1976d2;
  border-color: rgba(25, 118, 210, 0.5);
}
.mui-button.mui-button-primary.mui-button-outlined.mui-button-hover {
  border-color: #1976d2;
  background-color: rgba(25, 118, 210, 0.08);
}
.mui-button.mui-button-primary.mui-button-text {
  color: #1976d2;
}
.mui-button.mui-button-primary.mui-button-text.mui-button-hover {
  background-color: rgba(25, 118, 210, 0.08);
}
.mui-button.mui-button-secondary {
  color: #fff;
  background-color: #dc004e;
}
.mui-button.mui-button-secondary.mui-button-hover {
  background-color: #9a0036;
}
.mui-button.mui-button-secondary.mui-button-outlined {
  color: #dc004e;
  border-color: rgba(220, 0, 78, 0.5);
  box-shadow: none;
}
.mui-button.mui-button-secondary.mui-button-outlined.mui-button-hover {
  border-color: #dc004e;
  background-color: rgba(220, 0, 78, 0.08);
}
.mui-button.mui-button-secondary.mui-button-text {
  color: #dc004e;
}
.mui-button.mui-button-secondary.mui-button-text.mui-button-hover {
  background-color: rgba(220, 0, 78, 0.08);
}
.mui-button.mui-button-outlined {
  background-color: transparent;
  box-shadow: none;
  border-width: 1px;
  border-style: solid;
}
.mui-button.mui-button-outlined.mui-button-hover {
  text-decoration: none;
}
.mui-button.mui-button-text {
  background-color: transparent;
  border: 0;
  box-shadow: none;
}
.mui-button.mui-button-text.mui-button-hover {
  text-decoration: none;
}
.mui-button.mui-button-inherit {
  color: inherit;
  border-color: currentColor;
}
.mui-button.mui-button-medium {
  padding: 6px 16px;
  font-size: 15px;
  height: 36px;
}
.mui-button.mui-button-medium.mui-button-circle {
  height: 48px;
  width: 48px;
}
.mui-button.mui-button-small {
  padding: 4px 10px;
  font-size: 13px;
  height: 30px;
}
.mui-button.mui-button-small.mui-button-circle {
  height: 40px;
  width: 40px;
}
.mui-button.mui-button-large {
  padding: 8px 22px;
  font-size: 16px;
  height: 42px;
}
.mui-button.mui-button-large.mui-button-circle {
  height: 56px;
  width: 56px;
}
.mui-button.mui-button-circle {
  padding: 0;
  border-radius: 50%;
  min-width: 0;
  min-height: 36px;
}
.mui-button .mui-button-label {
  width: 100%;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  line-height: 1.75;
  color: inherit;
}
.mui-button .mui-button-label::after,
.mui-button .mui-button-label::before {
  box-sizing: inherit;
}
.mui-button .mui-button-progress {
  position: absolute;
  top: 50%;
  left: 50%;
}
