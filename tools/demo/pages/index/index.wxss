.bottomNavigation.mui-bottom-navigation {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.045);
  border-top: 1px solid rgba(0, 0, 0, 0.08);
}

[data-momoui-theme=dark] .bottomNavigation.mui-bottom-navigation {
  background-color: #424242;
  border-top-color: rgba(255, 255, 255, 0.08);
}