<view style="padding: 12px 24px;margin: auto;display: flex;outline: 0;border-radius: 4px;justify-content: center;background-color: #f5f5f5;">
  <mui-button-group>
    <mui-button size="small" ripple="{{true}}" opentype="getPhoneNumber" bind:getphonenumber="getPhoneNumber">DEFAULT</mui-button>
    <mui-button size="small" color="primary" ripple="{{true}}" opentype="getUserInfo" bind:getuserinfo="getUserInfo">PRIMARY</mui-button>
    <mui-button size="small" color="secondary" ripple="{{true}}" opentype="openSetting" bind:opensetting="openSetting">SECONDARY</mui-button>
  </mui-button-group>
</view>
<view style="padding: 12px 24px;margin: auto;display: flex;outline: 0;border-radius: 4px;justify-content: center;background-color: #f5f5f5;">
  <mui-button size="small" style="margin-right: 6px;" variant="text" ripple="{{true}}" opentype="launchApp" bind:error="launchAppError">DEFAULT</mui-button>
  <mui-button size="small" style="margin-right: 6px;" variant="text" color="primary" ripple="{{true}}">PRIMARY</mui-button>
  <mui-button size="small" style="margin-right: 6px;" variant="text" color="secondary" ripple="{{true}}">SECONDARY</mui-button>
</view>
<view style="padding: 12px 24px;margin: auto;display: flex;outline: 0;border-radius: 4px;justify-content: center;background-color: #f5f5f5;">
  <mui-button size="small" style="margin-right: 6px;" variant="outlined" ripple="{{true}}">DEFAULT</mui-button>
  <mui-button size="small" style="margin-right: 6px;" variant="outlined" color="primary" ripple="{{true}}">PRIMARY</mui-button>
  <mui-button size="small" style="margin-right: 6px;" variant="outlined" color="secondary" ripple="{{true}}">SECONDARY</mui-button>
</view>