<view
  data-momoui-theme="{{theme}}"
  id="wrapper"
  style="padding-top: 100px;min-height: 100px;width: 100%;position:relative;overflow: hidden;"
>
  <view>
    <view style="margin-bottom: 12px;">
      <mui-button-group color="primary" variant="contained" m-style="margin: 0 12px">
        <mui-button bind:tap="gototestpage">ONE</mui-button>
        <mui-button disabled="{{true}}">TWO</mui-button>
        <mui-button>INJECT</mui-button>
        <mui-button>THREE</mui-button>
      </mui-button-group>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button-group color="primary" m-style="margin: 0 12px">
        <mui-button>ONE</mui-button>
        <mui-button>TWO</mui-button>
        <mui-button disabled="{{true}}">INJECT</mui-button>
        <mui-button>THREE</mui-button>
      </mui-button-group>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button-group color="primary" variant="text" m-style="margin: 0 12px">
        <mui-button>ONE</mui-button>
        <mui-button>TWO</mui-button>
        <mui-button>INJECT</mui-button>
        <mui-button disabled="{{true}}">THREE</mui-button>
      </mui-button-group>
    </view>
  </view>
  <view style="margin-bottom: 12px;">
    <mui-button shape="circle" size="small">
      <mui-icon name="default" m-style="font-size:20px;" />
    </mui-button>
    <mui-button shape="circle" color="primary" disabled>
      <mui-icon name="default" m-style="font-size:20px;" />
    </mui-button>
    <mui-button shape="circle" color="secondary" size="large">
      <mui-icon name="default" m-style="font-size:20px;" />
    </mui-button>
  </view>
  <view style="margin-bottom: 12px;">
    <mui-button shape="circle" size="small" variant="contained">
      <mui-icon name="progress-circle" m-style="font-size:20px;" />
    </mui-button>
    <mui-button shape="circle" color="primary" variant="contained">
      <mui-icon name="progress-circle" m-class="icon" />
    </mui-button>
    <mui-button shape="circle" color="secondary" size="large" variant="contained">
      <mui-icon name="progress-circle" m-class="icon" />
    </mui-button>
  </view>
  <view style="margin-bottom: 12px;">
    <mui-button shape="extended" size="small" variant="contained">
      <mui-icon m-style="margin-right:8px;font-size:20px;" name="search" />
      EXTENDED
    </mui-button>
    <mui-button shape="extended" color="primary" variant="contained">
      <mui-icon m-style="margin-right:8px;" name="search" m-class="icon" />
      EXTENDED
    </mui-button>
    <mui-button shape="extended" color="secondary" size="large" variant="contained">
      <mui-icon m-style="margin-right:8px;" name="search" m-class="icon" />
      EXTENDED
    </mui-button>
  </view>
  <view>
    <view style="margin-bottom: 12px;">
      <mui-button
        size="small"
      >TEXT</mui-button>
      <mui-button
        variant="outlined"
        size="small"
      >OUTLINED</mui-button>
      <mui-button size="small" variant="contained">CONTAINED</mui-button>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button
        color="primary"
        size="small"
      >TEXT</mui-button>
      <mui-button
        color="{{buttonColor}}"
        variant="outlined"
        size="small"
      >OUTLINED</mui-button>
      <mui-button
        color="primary"
        size="small"
        variant="contained"
        bind:tap="changeButtonColor"
        bind:longpress="testLongPress"
      >CONTAINED</mui-button>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button
        color="secondary"
        size="small"
      >TEXT</mui-button>
      <mui-button
        disabled="{{disabled}}"
        color="secondary"
        variant="outlined"
        size="small"
      >OUTLINED</mui-button>
      <mui-button
        color="secondary"
        size="small"
        variant="contained"
        bind:tap="changeDisabled"
      >CONTAINED</mui-button>
    </view>
  </view>
  <view>
    <view style="margin-bottom: 12px;">
      <mui-button
      >TEXT</mui-button>
      <mui-button
        variant="outlined"
      >OUTLINED</mui-button>
      <mui-button disabled="{{true}}" variant="contained">CONTAINED</mui-button>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button
        color="primary"
      >TEXT</mui-button>
      <mui-button
        color="primary"
        variant="outlined"
      >OUTLINED</mui-button>
      <mui-button
        variant="contained"
        color="primary"
      >CONTAINED<mui-icon size="20" name="trash" m-style="margin-left: 8px" color="#fff" /></mui-button>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button
        color="secondary"
      >TEXT</mui-button>
      <mui-button
        color="secondary"
        variant="outlined"
      >OUTLINED</mui-button>
      <mui-button
        color="secondary"
        variant="contained"
      >CONTAINED</mui-button>
    </view>
  </view>
    <view>
    <view style="margin-bottom: 12px;">
      <mui-button
        size="large"
      >TEXT</mui-button>
      <mui-button
        variant="outlined"
        size="large"
      >OUTLINED</mui-button>
      <mui-button size="large" variant="contained">CONTAINED</mui-button>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button
        color="primary"
        disabled="{{true}}"
        size="large"
      >TEXT</mui-button>
      <mui-button
        color="primary"
        variant="outlined"
        size="large"
      >OUTLINED</mui-button>
      <mui-button
        color="primary"
        loading="{{true}}"
        size="large"
        variant="contained"
      >CONTAINED</mui-button>
    </view>
    <view style="margin-bottom: 12px;">
      <mui-button
        color="secondary"
        size="large"
      >TEXT</mui-button>
      <mui-button
        color="secondary"
        variant="outlined"
        size="large"
      >OUTLINED</mui-button>
      <mui-button
        color="secondary"
        size="large"
        loading="{{true}}"
        variant="contained"
      >CONTAINED</mui-button>
    </view>
  </view>
</view>
