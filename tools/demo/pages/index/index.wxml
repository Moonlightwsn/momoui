<mui-app-bar
  color="primary"
  position="fixed"
  m-style="box-shadow: 0px 0px 0px 0px rgba(0,0,0,0), 0px 4px 5px 0px rgba(0,0,0,0.14), 0px 0px 0px 0px rgba(0,0,0,0);"
>
  <mui-toolbar variant="dense" disable-gutters="{{true}}">
    <mui-input
      style="flex-grow: 1;margin-left: 16px;"
      m-class="search-input"
      fullWidth="{{true}}"
      start-adornment="search"
      start-icon-color="#ffffff"
      inherit="{{true}}"
      placeholder="搜索"
      placeholder-style="color:#8dbbe9;"
    />
    <mui-button
      shape="circle"
      variant="text"
      icon="plus"
      custom-icon-color="#ffffff"
      custom-ripple-background-color="#ffffff"
      style="margin-left: 16px;margin-right: 4px;"
      m-style="height:40px;width:40px;"
    />
  </mui-toolbar>
</mui-app-bar>
<view style="margin-top: 48px;background-color: #f5f5f5;padding: 16px 8px;">
  <mui-paper m-style="padding: 24px 16px;">
    <mui-typography variant="h5">
      This is a sheet of paper.
    </mui-typography>
    <mui-typography variant="p">
      Paper can be used to build surface or other elements for your application.
    </mui-typography>
  </mui-paper>
  <mui-menu
    style="margin-top: 8px;display:inline-flex;"
    menu-items="{{menuItems}}"
    bind:itemClick="itemClick"
    menu-height="80"
  />
  <mui-dropdown placeholder="topLeft">
    <mui-menu
      menu-items="{{menuItems}}"
      bind:itemClick="itemClick"
      slot="overlay"
    />
    <mui-button
      shape="circle"
      variant="text"
      icon="plus"
      color="primary"
    />
  </mui-dropdown>
</view>
<mui-bottom-navigation
  style="position: fixed;bottom: 0; width: 100%;border-top: 1px solid #00000014;"
  color="primary"
  actions="{{actions}}"
  activeKey="{{activeKey}}"
  controlled="{{true}}"
  bind:change="bnchange"
/>
