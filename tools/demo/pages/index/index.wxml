<view style="padding: 12px 24px;margin: auto;display: flex;outline: 0;border-radius: 4px;justify-content: center;background-color: #f5f5f5;">
  <mui-button size="small" style="margin-right: 6px;" bind:tap="testTap">DEFAULT</mui-button>
  <mui-button size="small" style="margin-right: 6px;" variant="outlined" color="primary" bind:tap="testTap">PRIMARY</mui-button>
  <mui-button size="small" style="margin-right: 6px;" color="secondary" bind:tap="testTap">SECONDARY</mui-button>
</view>
<view style="padding: 12px 24px;margin: auto;display: flex;outline: 0;border-radius: 4px;justify-content: center;background-color: #f5f5f5;">
