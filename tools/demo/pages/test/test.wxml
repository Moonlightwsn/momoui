<view>
  <mui-test
    color="{{color}}"
    size="{{size}}"
    disabled="{{disabled}}"
  >
    TEST
  </mui-test>
  <button bindtap="trigger">切换</button>
  <mui-checkbox
    color="{{color}}"
    size="{{size}}"
    disabled="{{disabled}}"
  >Checkbox</mui-checkbox>
  <mui-radio
    color="{{color}}"
    size="{{size}}"
    disabled="{{disabled}}"
  >Radio</mui-radio>
  <mui-switch
    color="{{color}}"
    size="{{size}}"
    disabled="{{disabled}}"
  >Switch</mui-switch>
  <mui-testcheck
    color="{{color}}"
    size="{{size}}"
    disabled="{{disabled}}"
  >TestCheck</mui-testcheck>
</view>