<view data-momoui-theme="{{theme}}" style="padding: 24px 12px 60px;">
  <mui-chip
    label="Basic"
  />
  <mui-chip
    label="Disabled"
    disabled
  />
  <mui-chip
    label="Clickable"
    avatar="M"
    onClick="{{chipClick}}"
  />
  <mui-chip
    label="Deletable"
    avatar="{{avatar}}"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    label="Clickable deletable"
    icon="user"
    onClick="{{chipClick}}"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    label="Custom delete icon"
    onClick="{{chipClick}}"
    onDelete="{{chipDelete}}"
    deleteIcon="check"
  />
  <mui-chip
    avatar="M"
    color="primary"
    clickable
    deleteIcon="check"
    label="Primary clickable"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    color="primary"
    clickable
    deleteIcon="check"
    label="Primary clickable"
    icon="user"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    color="primary"
    label="Deletable primary"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    avatar="M"
    color="secondary"
    clickable
    deleteIcon="check"
    label="Secondary clickable"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    color="secondary"
    clickable
    deleteIcon="check"
    label="Secondary clickable"
    icon="user"
    onDelete="{{chipDelete}}"
  />
  <mui-chip
    color="secondary"
    label="Deletable secondary"
    onDelete="{{chipDelete}}"
  />
