            }
            .mui-ripple {
              background-color: @primary_dark_color;
            }
          }
        }
      }
      &.mui-tabs-text-color-secondary {
        .mui-tab-root {
          &.selected-true {
            color: @secondary_dark_color;
            .mui-tab-wrapper {
              .mui-icon {
                color: @secondary_dark_color;
              }
            }
            .mui-ripple {
              background-color: @secondary_dark_color;
            }
          }
        }
      }
      .mui-tabs-flex-container {
        .mui-tabs-indicator {
          &.indicator-color-primary {
            background-color: @primary_dark_color;
          }
          &.indicator-color-secondary {
            background-color: @secondary_dark_color;
          }
        }
      }
    }

    &.mui-avatar-group {
      display: flex;
      .mui-avatar {
        border: 2px solid #121212;
      }
    }

    &.mui-avatar {
      &.avatar-color-default {
        color: #121212;
        background-color: #757575;
      }
    }

    &.mui-divider {
      background-color: rgba(255, 255, 255, 0.12);
      &.light-true {
        background-color: rgba(255, 255, 255, 0.08);
      }
    }

    &.mui-tooltip-root {
      .mui-paper-root {
        background-color: transparent;
      }
    }

    &.disabled-true {
      color: rgba(255, 255, 255, 0.3);
    }

    &.mui-color-default {
      color: #fff;
      border-color: rgba(255, 255, 255, 0.23);
    }

    &.mui-color-primary {
      color: @primary_dark_color;
      background-color: @primary_dark_color;
      border-color: rgba(144, 202, 249, 0.5);
    }

    &.mui-color-secondary {
      color: @secondary_dark_color;
      background-color: @secondary_dark_color;
      border-color: rgba(244, 143, 177, 0.5);
    }

    &.mui-variant-contained {
      color: rgba(0, 0, 0, 0.87);
    }

    &.mui-variant-outlined {
      background-color: transparent; 
    }

    &.mui-variant-text {
      background-color: transparent;
    }

    &.mui-form-group {
      &.disabled-true {
        color: rgba(255, 255, 255, 0.5);
      }
    }

    .mui-form-label {
      &.focus-false {
        color: rgba(255, 255, 255, 0.7);
      }
      &.mui-color-primary, &.mui-color-secondary {
        background-color: unset;
        border-color: unset;
      }
      &.disabled-true {
        color: rgba(255, 255, 255, 0.5);
      }
      &.error-true {
        color: @error_color;
      }
    }

    &.mui-form-helper-text {
      color: rgba(255, 255, 255, 0.7);
      &.disabled-true {
        color: rgba(255, 255, 255, 0.5);
      }
      &.error-true {
        color: @error_color;
      }
    }

    &.mui-progress-root {
      &.progress-color-primary {
        background-color: #48657c;
        .progress-bar-dashed {
          background-size: 10px 10px;
          background-image: radial-gradient(#48657c 0%, #48657c 16%, transparent 42%);
          background-position: 0 -23px;
        }
        .progress-bar2-dashed {
          background-color: #48657c;
        }
      }
      &.progress-color-secondary {
        background-color: #7a4758;
        .progress-bar-dashed {
          background-size: 10px 10px;
          background-image: radial-gradient(#7a4758 0%, #7a4758 16%, transparent 42%);
          background-position: 0 -23px;
        }
        .progress-bar2-dashed {
          background-color: #7a4758;
        }
      }
      &.progress-variant-buffer {
        background-color: transparent;
      }
    }

    &.mui-bottom-navigation {
      background-color: #424242;
    }

    &.mui-bottom-navigation-action {
      color: rgba(255, 255, 255, 0.7);
      .mui-bottom-navigation-action-wrapper {
        .mui-icon {
          color: rgba(255, 255, 255, 0.7);
        }
      }
      &.selected-true {
        color: @primary_dark_color;
        .mui-bottom-navigation-action-wrapper {
          .mui-icon {
            color: @primary_dark_color;
          }
        }
        .mui-ripple {
          background-color: @primary_dark_color;
        }
      }
    }


    &.mui-button {
      &.mui-icon-button {
        &.mui-color-default {
          color: #fff;
          &.disabled-true {
            color: rgba(255, 255, 255, 0.3);
          }
        }
      }
      &.mui-button-hover {
        background-color: #ffffff14;
        &.mui-color-primary {
          background-color: #90caf914;
        }
        &.mui-color-secondary {
          background-color: #f48fb114;
        }
        &.mui-variant-contained {
          background-color: #d5d5d5;
          &.mui-color-primary {
            background-color: @primary_dark_color_2;
          }
          &.mui-color-secondary {
            background-color: @secondary_dark_color_2;
          }
        }
        &.mui-variant-outlined {
          border-color: #ffffff3b;
          &.mui-color-primary {
            border-color: @primary_dark_color;
          }
          &.mui-color-secondary {
            border-color: @secondary_dark_color;
          }
        }
      }
      &.disabled-true {
        color: rgba(255, 255, 255, 0.3);
        &.mui-variant-contained {
          background-color: rgba(255, 255, 255, 0.12);
        }
        &.mui-variant-text {
          background-color: transparent;
        }
        &.mui-variant-outlined {
          background-color: transparent;
          border-color: rgba(255, 255, 255, 0.3);
        }
      }
    }

    &.mui-button-group {
      &.mui-variant-contained {
        background-color: transparent;
      }
      .mui-button {
        &.disabled-true {
          &.mui-variant-text, &.mui-variant-outlined, &.mui-variant-contained {
            border-left-color: rgba(255, 255, 255, 0.3);
            border-top-color: rgba(255, 255, 255, 0.3);
            border-right-color: rgba(255, 255, 255, 0.3);
            border-bottom-color: rgba(255, 255, 255, 0.3);
          }
        }
      }
    }

    &.mui-chip {
      color: #fff;
      background-color: #616161;
      .mui-chip-avatar {
        color: #e0e0e0;
      }
      .mui-chip-icon {
        color: #e0e0e0;
      }
      .mui-chip-delete {
        color: rgba(255, 255, 255, 0.26);
      }
      &.mui-color-primary {
        color: rgba(0, 0, 0, 0.87);
        background-color: @primary_dark_color;
        .mui-chip-avatar {
          background-color: @primary_dark_color_2;
          &.icon {
            background-color: @primary_dark_color;
          }
        }
      }
      &.mui-color-secondary {
        color: rgba(0, 0, 0, 0.87);
        background-color: @secondary_dark_color;
        .mui-chip-avatar {
          background-color: @secondary_dark_color_2;
          &.icon {
            background-color: @secondary_dark_color;
          }
        }
      }
      &.mui-color-primary, &.mui-color-secondary {
        .mui-chip-avatar {
          color: rgba(0, 0, 0, 0.87);
        }
        .mui-chip-icon {
          color: inherit;
        }
        .mui-chip-delete {
          color: rgba(0, 0, 0, 0.7);
        }
      }
      &.mui-variant-outlined {
        border: 1px solid rgba(255, 255, 255, 0.23);;
        background-color: transparent;
        .mui-chip-avatar {
          &.icon {
            background-color: transparent;
          }
        }
        &.mui-color-primary {
          color: @primary_dark_color;
          border: 1px solid @primary_dark_color;
          .mui-chip-avatar {
            &.icon {
              color: #90caf9b3;
            }
          }
          .mui-chip-delete {
            color: #90caf9b3;
          }
        }
        &.mui-color-secondary {
          color: @secondary_dark_color;
          border: 1px solid @secondary_dark_color;
          .mui-chip-avatar {
            &.icon {
              color: #f48fb1b3;
            }
          }
          .mui-chip-delete {
            color: #f48fb1b3;
          }
        }
      }
      &.disabled-true {
        background-color: #616161;
        &.mui-color-primary, &.mui-color-secondary {
          color: #fff;
        }
        &.mui-variant-outlined {
          border-width: 0;
        }
      }
    }

    &.mui-snack-bar-root .snack-bar-content-root, &.snack-bar-content-root {
      color: rgba(0, 0, 0, 0.87);
      background-color: rgb(250, 250, 250);
    }

    &.mui-checkbox, &.mui-radio, &.mui-switch {
      &.mui-color-default, &.mui-color-primary, &.mui-color-secondary {
        color: unset;
        background-color: unset;
        border-color: unset;
      }
      .mui-button {
        &.mui-color-default {
          color: #bdbdbd;
          .check-icon {
            &.switch-icon {
              color: #bdbdbd;
            }
          }
        }
      }
      &.mui-unchecked {
        .mui-button {
          .check-icon {
            color: #bdbdbd;
          }
        }
      }
      &.disabled-true {
        color: rgba(255, 255, 255, 0.5);
        .mui-button {
          .check-icon {
            color: #646464;
          }
        }
      }
    }
    &.mui-switch {
      .switch-root {
        .switch-track {
          opacity: 0.3;
          background-color: #fff;
        }
      }
      &.mui-checked {
        &.mui-color-secondary {
          .switch-track {
            background-color: @secondary_dark_color;
          }
        }
        &.mui-color-primary {
          .switch-track {
            background-color: @primary_dark_color;
          }
        }
      }
      &.disabled-true {
        .switch-track {
          opacity: 0.1;
        }
        &.mui-unchecked {
          .mui-button {
            .check-icon {
              &.switch-icon {
                color: #646464;
              }
            }
          }
        }
        &.mui-color-secondary, &.mui-color-primary {
          .switch-track {
            background-color: #fff;
          }
        }
      }
    }
    
    &.mui-input-root {
      &.disable-underline-false {
        &::before {
          border-bottom-color: rgba(255, 255, 255, 0.7);
        }
        &::after {
          border-bottom-color: @primary_dark_color;
        }
      }
      &.mui-color-primary {
        color: #fff;
        background-color: unset;
        border-color: unset;
        &.disable-underline-false {
          &::after {
            border-bottom-color: @primary_dark_color;
          }
        }
        &.focus-true {
          &.mui-variant-outlined {
            border-color: @primary_dark_color;
          }
        }
      }
      &.mui-color-secondary {
        color: #fff;
        background-color: unset;
        border-color: unset;
        &.disable-underline-false {
          &::after {
            border-bottom-color: @secondary_dark_color;
          }
        }
        &.focus-true {
          &.mui-variant-outlined {
            border-color: @secondary_dark_color;
          }
        }
      }
      &.mui-variant-filled {
        background-color: rgba(255, 255, 255, 0.09);
      }
      &.mui-variant-outlined {
        border-color: rgba(255, 255, 255, 0.23);
      }
      &.error-true {
        &.mui-color-primary, &.mui-color-secondary {
          &.mui-variant-outlined {
            border-color: @error_color;
          }
          &::after {
            border-bottom-color: @error_color;
          }
        }
      }
      &.disabled-true {
        color: rgba(255, 255, 255, 0.5);
        &.mui-color-primary, &.mui-color-secondary {
          color: rgba(255, 255, 255, 0.5); 
        }
        &.mui-select-input {
          .select-arrow {
            color: rgba(255, 255, 255, 0.5); 
          }
        }
        .mui-input {
          opacity: 1;
        }
      }
      &.error-true {
        color: @error_color;
      }
      /*
      // textarea  placeholder  
      .mui-input-placeholder {
        color: #959595;
      }
      */
    }

    &.mui-input-label {
      color: rgba(255, 255, 255, 0.7);
      &.mui-color-primary {
        background-color: unset;
        border-color: unset;
        &.focus-true {
          color: @primary_dark_color;
        }
      }
      &.mui-color-secondary {
        background-color: unset;
        border-color: unset;
        &.focus-true {
          color: @secondary_dark_color;
        }
      }
      &.error-true {
        color: @error_color;
        &.mui-color-primary, &.mui-color-secondary {
          color: @error_color;
        }
      }
      &.disabled-true {
        color: rgba(255, 255, 255, 0.5);
        &.mui-color-primary, &.mui-color-secondary {
          color: rgba(255, 255, 255, 0.5); 
        }
      }
    }

    &.mui-typography {
      &.mui-color-primary, &.mui-color-secondary {
        background-color: unset;
        border-color: unset;
      }
      &.mui-color-textPrimary {
        color: #fff;
      }
      &.mui-color-textSecondary {
        color: rgba(255, 255, 255, 0.7);
      }
    }

    &.mui-alert {
      &.mui-variant-outlined {
        background-color: transparent;
      }
      &.mui-color-error {
        &.mui-variant-standard {
          color: #fab3ae;
          background-color: #180605;
        }
        &.mui-variant-outlined {
          color: #fab3ae;
          border: 1px solid @error_color;
        }
      }
      &.mui-color-warning {
        &.mui-variant-standard {
          color: #ffd599;
          background-color: #190f00;
        }
        &.mui-variant-outlined {
          color: #ffd599;
          border: 1px solid @warning_color;
        }
      }
      &.mui-color-info {
        &.mui-variant-standard {
          color: #a6d5fa;
          background-color: #030e18;
        }
        &.mui-variant-outlined {
          color: #a6d5fa;
          border: 1px solid @info_color;
        }
      }
      &.mui-color-success {
        &.mui-variant-standard {
          color: #b7dfb9;
          background-color: #071107;
        }
        &.mui-variant-outlined {
          color: #b7dfb9;
          border: 1px solid @success_color;
        }
      }
    }

    &.mui-list-item {
      &.divider-true {
        border-bottom: 1px solid rgba(255, 255, 255, 0.12);
        background-clip: padding-box;
      }
      &.mui-list-item-hover {
        text-decoration: none;
        background-color: rgba(255, 255, 255, 0.08);
      }
      &.selected-true {
        background-color: rgba(255, 255, 255, 0.16);
      }
    }
    
    &.mui-list-item-icon {
      color: #fff;
    }

    &.mui-list-subheader {
      color: rgba(255, 255, 255, 0.7);
    }
  }
}

@media (prefers-color-scheme: dark) {
  page {
    background-color: #212121;
  }
}

button:not([size='mini']) {
  width: unset;
}