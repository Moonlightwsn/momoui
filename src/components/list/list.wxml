<view
  class="mui mui-list disable-padding-{{disablePadding}} dense-{{dense}} nested-{{nested}}{{subheader ? ' list-subheader ' : ' '}}{{mClass}}"
  style="{{mStyle}}"
>
  <mui-list-subheader wx:if="{{subheader}}">
    {{subheader}}
  </mui-list-subheader>
  <slot wx:else name="subheader" />
  <slot />
</view>