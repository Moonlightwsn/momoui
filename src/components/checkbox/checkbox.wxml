<view
  class="mui mui-checkbox {{muiClasses}}"
  style="{{mStyle}}"
  capture-bind:tap="_checkControll"
  capture-bind:longpress="_checkControll"
>
  <mui-button disabled="{{disabled}}" ripple="{{ripple}}" color="{{color}}" shape="circle" variant="text" m-style="padding: 9px;">
    <mui-icon name="{{_currentIcon}}" m-style="{{_currentIconStyle}}" />
  </mui-button>
  <slot></slot>
</view>
