.mui {
  &.mui-button {
    display: inline-flex;
    cursor: pointer;
    &.mui-shape-normal {

    }
    &.mui-shape-circle {
      border-radius: 50%;
      padding: 14px;
    }
    &.mui-shape-extended {
      border-radius: 24px;
    }
    &::after {
      border: 0;
      border-radius: 0;
    }
  }
}