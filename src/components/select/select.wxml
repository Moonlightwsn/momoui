<picker
  custom-item="{{customItem}}"
  disabled="{{disabled}}"
  end="{{end}}"
  fields="{{fields}}"
  headerText="{{headerText}}"
  value="{{value}}"
  mode="{{mode}}"
  range="{{range}}"
  range-key="{{rangeKey}}"
  start="{{start}}"
  bindchange="_change"
>
  <mui-input
    disabled
    m-class="mui-select select-disabled-{{disabled}} {{mClass}}"
    m-style="{{mStyle}}"
    value="{{_display}}"
  />
</picker>