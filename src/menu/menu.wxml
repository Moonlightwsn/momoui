<mui-paper
  class="mui mui-menu {{mClass}}"
  style="{{mStyle}}"
>
  <view
    class="mui-menu-ul"
  >
    <mui-button
      wx:for="{{menuItems}}"
      wx:key="{{item.key || index}}"
      m-class="mui-menu-item"
      data-key="{{item.key || index}}"
      variant="text"
    >
      {{item.text || ''}}
    </mui-button>
  </view>
</mui-paper>